<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>user_Add</servlet-name>
        <servlet-class>Servlet.user_Add</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>user_Add</servlet-name>
        <url-pattern>/user/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>userFind</servlet-name>
        <servlet-class>Servlet.userFind</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userDeleteServlet</servlet-name>
        <servlet-class>Servlet.userDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userDeleteServlet</servlet-name>
        <url-pattern>/user/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userFind</servlet-name>
        <url-pattern>/user/find</url-pattern>
    </servlet-mapping>
</web-app>